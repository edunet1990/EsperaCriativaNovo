
@{
    ViewBag.Title = "Profile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Perfil do usuário
        </h1>
       
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-lg-offset-2 col-md-7">
                <!-- Profile Image -->
                <div class="box">
                    <div class="box-body box-profile">
                        <a href="#" class="btn btn-md btn-primary "><b>Editar</b></a>
                        <img class="profile-user-img img-responsive img-circle" src="~/Content/adminlte/img/user2-160x160.jpg" alt="User profile picture">
                        <h3 class="profile-username text-center">Alexandre </h3>
                        <p class="text-muted text-center">Professor</p>
                        <ul class="list-group list-group-unbordered">
                            <li class="list-group-item">
                                <b>Nome</b> <a class="pull-right">Alexandre Pierece dos Santos</a>
</li>
                            <li class="list-group-item">
                                <b>Cargo</b> <a class="pull-right">Professor</a>
                            </li>
                            <li class="list-group-item">
                                <b>Escola</b> <a class="pull-right">CE Manuel</a>
                            </li>
                        </ul>
                    </div>
                    <!-- /.box-body -->
                </div>
                <!-- /.box -->
                <!-- /.col -->

            </div>
            <!-- /.row -->
    </section>
    <!-- /.content -->
</div>
<!-- /.content-wrapper -->
